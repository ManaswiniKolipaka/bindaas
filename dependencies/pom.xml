			<name>Spring Plugins Repository</name>
			<url>http://repo.spring.io/plugins-release/</url>
		</repository>
		<repository>
			<id>jabylon</id>
			<name>Jabylon Repository</name>
			<url>http://www.jabylon.org/maven/</url>
		</repository>
		<repository>
			<id>geomajas</id>
			<name>Geomajas Repository</name>

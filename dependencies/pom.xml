							<destFileName>org.eclipse.osgi_3.10.100.v20150529-1857.jar</destFileName>
						</artifactItem>
						<artifactItem>
							<groupId>org.osgi</groupId>
							<artifactId>org.eclipse.osgi</artifactId>
